<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="CardContainer">
        <attr name="android:gravity" />
        <attr name="cardContainerOrientation" format="enum">
            <enum name="vertical" value="0" />
            <enum name="horizontal" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="HoloCircularProgressBar">
        <attr name="hcp_stroke_width" format="dimension" />
        <attr name="hcp_hcp_progress" format="float" />
        <attr name="hcp_marker_progress" format="float" />
        <attr name="hcp_progress_color" format="color" />
        <attr name="hcp_progress_background_color" format="color" />
        <attr name="hcp_thumb_visible" format="boolean" />
        <attr name="hcp_marker_visible" format="boolean" />
        <attr name="android:gravity" />
    </declare-styleable>
    <attr name="hcpCircularProgressBarStyle" format="reference" />
    <attr name="hcp_cpb_progress" format="float" />

    <declare-styleable name="SpanVariableGridView">
        <attr name="SpanVariableGridView_numColumns" format="integer" />
        <attr name="SpanVariableGridView_itemMargin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CreditsRollView">
        <attr name="crv_scrollPosition" format="float" />
        <attr name="crv_angle" format="float" />
        <attr name="crv_endScrollMultiplier" format="float" />
        <attr name="crv_distanceFromText" format="float" />
    </declare-styleable>


    <attr name="cropImageStyle" format="reference" />

    <declare-styleable name="CropImageView">
        <attr name="crop_highlightColor" format="reference|color" />
        <attr name="crop_showThirds" format="boolean" />
        <attr name="crop_showHandles">
            <enum name="changing" value="0" />
            <enum name="always" value="1" />
            <enum name="never" value="2" />
        </attr>
    </declare-styleable>


    <declare-styleable name="CropperImageView">
        <attr name="crop_guidelines">
            <enum name="on" value="2" />
            <enum name="onTouch" value="1" />
            <enum name="off" value="0" />
        </attr>
        <attr name="crop_fixAspectRatio" format="boolean" />
        <attr name="crop_aspectRatioX" format="integer" />
        <attr name="crop_aspectRatioY" format="integer" />
        <attr name="crop_imageResource" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomPullRefreshLayout">
        <attr name="cprlType" format="enum">
            <enum name="circles" value="0" />
            <enum name="water_drop" value="1" />
            <enum name="ring" value="2" />
        </attr>
        <attr name="cprlColors" format="reference" />
    </declare-styleable>


    <declare-styleable name="DiscrollView_LayoutParams">
        <attr name="discrollve_alpha" format="boolean" />
        <attr name="discrollve_scaleX" format="boolean" />
        <attr name="discrollve_scaleY" format="boolean" />
        <attr name="discrollve_threshold" format="float" />
        <attr name="discrollve_fromBgColor" format="color" />
        <attr name="discrollve_toBgColor" format="color" />
        <attr name="discrollve_translation">
            <flag name="fromTop" value="0x01" />
            <flag name="fromBottom" value="0x02" />
            <flag name="fromLeft" value="0x04" />
            <flag name="fromRight" value="0x08" />
        </attr>
    </declare-styleable>


    <declare-styleable name="DragSortListView">
        <attr name="dslv_collapsed_height" format="dimension"/>
        <attr name="dslv_drag_scroll_start" format="float"/>
        <attr name="dslv_max_drag_scroll_speed" format="float"/>
        <attr name="dslv_float_background_color" format="color"/>
        <attr name="dslv_remove_mode">
            <enum name="clickRemove" value="0"/>
            <enum name="flingRemove" value="1"/>
        </attr>
        <attr name="dslv_track_drag_sort" format="boolean"/>
        <attr name="dslv_float_alpha" format="float"/>
        <attr name="dslv_slide_shuffle_speed" format="float"/>
        <attr name="dslv_remove_animation_duration" format="integer"/>
        <attr name="dslv_drop_animation_duration" format="integer"/>
        <attr name="dslv_drag_enabled" format="boolean"/>
        <attr name="dslv_sort_enabled" format="boolean"/>
        <attr name="dslv_remove_enabled" format="boolean"/>
        <attr name="dslv_drag_start_mode">
            <enum name="onDown" value="0"/>
            <enum name="onMove" value="1"/>
            <enum name="onLongPress" value="2"/>
        </attr>
        <attr name="dslv_drag_handle_id" format="integer"/>
        <attr name="dslv_fling_handle_id" format="integer"/>
        <attr name="dslv_click_remove_id" format="integer"/>
        <attr name="dslv_use_default_controller" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="DraggableFlagView">
        <attr name="df_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="EdgeEffectView">
        <attr name="edgeeffect_color" format="color" />
    </declare-styleable>


    <declare-styleable name="FancyCoverFlow">
        <attr name="unselectedAlpha" format="float"/>
        <attr name="unselectedSaturation" format="float"/>
        <attr name="unselectedScale" format="float"/>
        <attr name="maxRotation" format="integer"/>
        <attr name="scaleDownGravity" format="float"/>
        <attr name="actionDistance" format="integer">
            <enum name="auto" value="2147483647"/>
        </attr>
    </declare-styleable>


    <!--FlipViews-->
    <declare-styleable name="FlipViewController">

        <attr name="orientation"></attr>

        <attr name="animationBitmapFormat" format="enum">
            <enum name="ARGB_8888" value="0"/>
            <enum name="ARGB_4444" value="1"/>
            <enum name="RGB_565" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="FlipView">
        <attr name="orientation"></attr>
        <attr name="overFlipMode" format="enum">
            <enum name="glow" value="0"/>
            <enum name="rubber_band" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="FoldingMenu">
        <attr name="foldNumber" format="integer"/>
    </declare-styleable>

    <declare-styleable name="PullScrollView">
        <attr name="psv_header" format="reference"/>
        <attr name="psv_headerHeight" format="dimension"/>
        <attr name="psv_headerVisibleHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="ListBuddiesOptions">
        <attr name="gap" format="reference"/>
        <attr name="gapColor" format="color"/>
        <attr name="speed" format="integer"/>
        <attr name="listsDivider" format="reference"/>
        <attr name="listsDividerHeight" format="reference"/>
        <attr name="autoScrollFaster" format="enum">
            <enum name="right" value="0"/>
            <enum name="left" value="1"/>
        </attr>
        <attr name="scrollFaster" format="enum">
            <enum name="right" value="0"/>
            <enum name="left" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ParallaxImageView">
        <attr name="pi_intensity" format="float"/>
        <attr name="tiltSensitivity" format="float"/>
        <attr name="forwardTiltOffset" format="float"/>
        <attr name="scaledIntensity" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="SelectorOptions">
        <attr name="so_selectorColor" format="color"/>
    </declare-styleable>


    <declare-styleable name="ShowcaseView">
        <attr name="sv_backgroundColor" format="color|reference"/>
        <attr name="sv_detailTextColor" format="color|reference"/>
        <attr name="sv_titleTextColor" format="color|reference"/>
        <attr name="sv_buttonBackgroundColor" format="color|reference"/>
        <attr name="sv_buttonForegroundColor" format="color|reference"/>
        <attr name="sv_buttonText" format="string|reference"/>
        <attr name="sv_detailTextAppearance" format="reference"/>
        <attr name="sv_titleTextAppearance" format="reference"/>
        <attr name="sv_showcaseColor" format="color|reference"/>
        <attr name="sv_tintButtonColor" format="boolean|reference"/>
    </declare-styleable>

    <declare-styleable name="ParallaxScroll">
        <attr name="parallax_factor" format="float"/>
        <attr name="inner_parallax_factor" format="float"/>
        <attr name="parallax_views_num" format="integer"/>
        <attr name="circular_parallax" format="boolean"/>
    </declare-styleable>



    <declare-styleable name="PullToZoomScrollView">
        <attr name="scrollHeadView" format="reference"/>
        <attr name="scrollContentView" format="reference"/>
        <attr name="scrollZoomView" format="reference"/>
    </declare-styleable>

    <declare-styleable name="PullToZoomListView">
        <attr name="listHeadView" format="reference"/>
        <attr name="isHeadParallax" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="CustomTheme">
        <attr name="showcaseViewStyle" format="reference"/>
    </declare-styleable>

    <declare-styleable name="StaggeredGridView">
        <attr name="sg_column_count" format="integer"/>
        <attr name="sg_column_count_portrait" format="integer"/>
        <attr name="sg_column_count_landscape" format="integer"/>
        <attr name="sg_item_margin" format="dimension"/>
        <attr name="sg_grid_paddingLeft" format="dimension"/>
        <attr name="sg_grid_paddingRight" format="dimension"/>
        <attr name="sg_grid_paddingTop" format="dimension"/>
        <attr name="sg_grid_paddingBottom" format="dimension"/>
    </declare-styleable>


    <attr name="fontPath" format="string"/>


</resources>