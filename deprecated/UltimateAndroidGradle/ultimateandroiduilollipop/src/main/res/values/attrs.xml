<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GoogleProgressBar">
        <attr name="type" format="enum">
            <enum name="folding_circles" value="0"/>
            <enum name="google_music_dices" value="1"/>
            <enum name="nexus_rotation_cross" value="2"/>
        </attr>
        <attr name="colors" format="reference"/>
    </declare-styleable>


    <declare-styleable name="materialAnimatedSwitch">
        <attr name="mas_icon_release"
              format="reference"/>
        <attr name="mas_icon_press"
              format="reference"/>
        <attr name="mas_base_release_color"
              format="color"/>
        <attr name="mas_base_press_color"
              format="color"/>
        <attr name="mas_ball_release_color"
              format="color"/>
        <attr name="mas_ball_press_color"
              format="color"/>
    </declare-styleable>
</resources>